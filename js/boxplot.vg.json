{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "description": "A boxplot showing the distribution of finalWorth across different categories.",
    "data": {"url": "https://raw.githubusercontent.com/Yousuf-Monash/FIT3179-Assignment-2/main/data/Billionaires%20Statistics%20Dataset.csv"},
    "width": "container",
    "mark": "boxplot",
    "encoding": {
      "y": {
        "field": "category",
        "type": "nominal",
        "axis": {"title": "Category"}
      },
      "x": {
        "title": "Net Worth",
        "field": "finalWorth",
        "type": "quantitative",
        "scale": {"type": "log"}
      },
      "color": {
        "field": "category",
        "type": "nominal",
        "legend": {"title": "Category"}
      }
    },
    "config": {
      "boxplot": {
        "extent": 2
      },
      "background": "#dff7f7",
      "font": "PT Serif",
      "padding": {"top": 10, "bottom": 10, "left": 30, "right": 30},
      "view": {"stroke": null},
      "legend": {
        "titleFontSize": 12,
        "titleColor": "gray",
        "labelColor": "gray",
        "direction": "vertical",
        "orient": "right",
        "gradientLength": 180,
        "gradientThickness": 10,
        "gradientLabelOffset": 5,
        "offset": 20
      },
      "text": {"fontSize": 13},
      "title": {"fontSize": 20, "offset": 5}
    }
  }
  